/* Includes ------------------------------------------------------------------*/
#include <API_SPI.h>
#include <stdio.h>

/**
 * @brief  Initialize ADS1293 device
 * @param  none
 * @retval bool Init process status
 */
bool_t ads1293Init(void) {
	/* Init SPI peripheral */
	if (!spiInit(0)) {
		return false;
	}
	return true;
}
